 <% @blogs.each do |blog| %>
   <div class="blogbox">
   <h1><%= blog.title%></h1>
    <p><%= blog.text%></p>
     <ul class="btn">
          <li>
            <%= link_to "SHOW", blog_path(blog) %>
          </li>
          <li>
            <%= link_to "EDIT", edit_blog_path(blog) %>
          </li>
          <li>
            <%= link_to "DESTROY", blog_path(blog), method: :delete %>
          </li>
      </ul>
   </div>
<% end %>
